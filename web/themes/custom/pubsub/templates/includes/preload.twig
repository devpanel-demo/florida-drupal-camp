{#
/**
 * @file
 * Preload the non-bold & non-italic fonts for the headings and the body copy.
 *
 * Available variables:
 * - pubsub_path: Returns the path to the pubsub theme.
 */
#}

<link rel="preload" href="{{ pubsub_path }}/fonts/general-sans/GeneralSans-Regular.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ pubsub_path }}/fonts/general-sans/GeneralSans-Variable.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ pubsub_path }}/fonts/general-sans/GeneralSans-Semibold.woff2" as="font" type="font/woff2" crossorigin>
