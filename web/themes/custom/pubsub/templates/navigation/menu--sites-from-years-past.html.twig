{{ attach_library('pubsub/archive-sites-menu') }}

  {% if items %}
    <select aria-label="Sites from years past" {{ attributes.addClass('archive-sites-menu', primary_nav_level) }}>
      <option selected>Sites from years past</option>
      {% for item in items %}
        <option data-url="{{ item.url }}">
          {{ item.title }}
        </option>
      {% endfor %}
    </select>
  {% endif %}
